  author = {Crippa, A. and Ezzouch, R. and Apr{\'a}, A. and Amisse, A. and Lavi{\'e}ville, R. and Hutin, L. and Bertrand, B. and Vinet, M. and Urdampilleta, M. and Meunier, T. and Sanquer, M. and Jehl, X. and Maurand, R. and De Franceschi, S.},
