}

@article{patraCryoCMOSCircuitsSystems2018,
  title = {Cryo-{{CMOS Circuits}} and {{Systems}} for {{Quantum Computing Applications}}},
  author = {Patra, Bishnu and Incandela, Rosario M. and {van Dijk}, Jeroen P. G. and Homulle, Harald A. R. and Song, Lin and Shahmohammadi, Mina and Staszewski, Robert Bogdan and Vladimirescu, Andrei and Babaie, Masoud and Sebastiano, Fabio and Charbon, Edoardo},
