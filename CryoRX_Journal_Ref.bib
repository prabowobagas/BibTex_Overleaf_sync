  langid = {english},
  keywords = {Electronic and spintronic devices,Qubits},
  annotation = {Bandiera\_abtest: a Cc\_license\_type: cc\_by Cg\_type: Nature Research Journals Primary\_atype: Reviews Subject\_term: Electronic and spintronic devices;Qubits Subject\_term\_id: electronic-and-spintronic-devices;qubits},
