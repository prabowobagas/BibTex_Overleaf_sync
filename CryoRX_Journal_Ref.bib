  langid = {english},
  keywords = {Quantum information,Quantum physics},
  annotation = {Bandiera\_abtest: a Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Quantum information;Quantum physics Subject\_term\_id: quantum-information;quantum-physics},
  file = {C\:\\Users\\Bagas Prabowo\\Zotero\\storage\\AP5Y2EYL\\Arute et al. - 2019 - Quantum supremacy using a programmable superconduc.pdf;C\:\\Users\\Bagas Prabowo\\Zotero\\storage\\KQPFI6UW\\s41586-019-1666-5.html}
}

@article{bardinDesignCharacterization28nm2019,
